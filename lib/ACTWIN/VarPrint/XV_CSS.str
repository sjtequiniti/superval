    /*     *  CSS rules for Varprints and Category Audit files     *  [ExcelVarprint] MAINWIN#8 SJT 22feb10     */    * {margin: 0; padding: 0}    /* TYPE */    body        {font-family: Verdana, sans-serif}    pre, p.err        {font-family: "Courier New", Courier, fixed}    /* LEADING */    p, pre, table {margin-bottom: 0.8em}    h1, h2   {margin-bottom: 0.3em}    @media print {    		a {text-decoration: none}        body, table {font-size: 6pt}        table {page-break-inside: avoid}        #toc-controls {display: none}        #toc {position: relative} /* reference frame for children */        #toc p {font-size: 24pt}        #toc dl {            /* background image not yet implemented by IE for print media (sigh) */        	background: url("http://lambenttechnology.com/claybrook/setup.bmp") no-repeat 0 1in;        	padding-bottom: 3.5in;        	position: absolute; top: .75in; left: 6in;        	width: 5in;        }        #toc dt {color: gray}        #toc dd {        	font-weight: bold;        	margin-left: 36pt;        }        #toc ul {        	margin: 24pt 0 0 36pt;        	list-style: none;        }        #toc ul ul {margin: 6pt 0 0 6pt}        #toc li {margin-bottom: 6pt}    }    @media screen {        body, table {font-size: 75%}        body {padding:0 1em 1em 1em}        body.cataudit {padding-left: 18em} /* space for ToC */        #toc-controls {        	padding: 0.3em 0.5em 0;        	position: fixed; top: 0.3em; left: 0.3em;        	z-index: 3;        }        #toc-controls button {        	font-size: 0.7em;        	letter-spacing: 0.1em;        	text-transform: uppercase;        }        #toc {          background: #DDD;          border: 1px solid #AAA;          padding: 3em 1em 1em;        	position: fixed; top: 0.3em; left: 0.3em;        	width: 15em;        }        #toc dl, #toc p  {margin-bottom: 0.8em}        #toc dd {margin: 0 0 0.3em 1em}        #toc dt {color: gray}        #toc li {        	font-size: 0.9em;        	list-style: none;        	margin: 0.2em 0 0.8em 0;            }        #toc ul {margin-top: 0.3em}        #toc ul ul {margin-left: 1em}        #toc a {text-decoration: none}        #toc a:hover {color: red}				.print i{display: none}    }    caption {        background: #353;        color: #DFD;        font-size: 1.4em;        text-align: left;    }    h1 {        color: #353;        font-size: 1.6em;        margin-top: 1em;        page-break-after: avoid;        page-break-before: always;    }    h2 {        color: #575;        font-size: 1.2em;        margin-top: 0.5em;        page-break-after: avoid;    }    table {border-collapse: collapse}    td, th {padding: 0 0.5em}    /* IE: repeating table headers on printed pages */    thead {display: table-header-group}    /* CLASSES */    .err {        background: #FDD;        color: red;        padding: 0.5em;        white-space: pre;    }    .lbl {        border-left: 1px solid gray;        color: gray;    }    .star {background: #FFD}    .val {        color: black;        text-align: left; /* for Excel */    }